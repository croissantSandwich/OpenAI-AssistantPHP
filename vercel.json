{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.1"
    },
    "controller.php": {
      "runtime": "vercel-php@0.6.1"
    }
  },
  "routes": [
    {
      "src": "/controller.php",
      "methods": ["GET", "POST"],
      "dest": "/controller.php"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}